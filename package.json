    "karma-sauce-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.7.0",
    "lite-server": "^2.2.2",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "pre-commit": "1.1.3",
    "protractor": "^4.0.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.3",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "stylus-loader": "1.5.1",
    "to-string-loader": "1.1.4",
    "ts-loader": "0.8.2",
    "tslint-config-valorsoft": "1.0.3",
    "typescript": "1.8.10",
    "typings": "1.3.1",
    "webpack": "1.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gapminder/gapminder-org.git"
  },
  "keywords": [
