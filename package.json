{
  "name": "gapminder.org",
  "version": "0.0.1",
  "private": true,
  "description": "gapminder.org angular2 client application",
  "main": "index.js",
  "engines": {
    "node": "4.6.0",
    "npm": "3.10.8"
  },
  "scripts": {
    "start": "node ./server",
    "heroku-postbuild": "npm run build",
    "start-dev": "./node_modules/.bin/concurrently \"npm run watch:dev\" \"npm run serve\"",
    "deploy": "npm run build:prod && npm run publish:gh-pages",
    "deploy:master": "npm run build:prod && ./node_modules/.bin/firebase deploy --project gapminder-org-stage",
    "deploy:testing": "npm run build && ./node_modules/.bin/firebase deploy --project gapminder-org-testing",
    "deploy:development": "npm run build && ./node_modules/.bin/firebase deploy",
    "publish:gh-pages": "./tools/gh-pages-publish.js",
    "build": "./node_modules/.bin/webpack --progress --color",
    "build:prod": "NODE_ENV=prod ./node_modules/.bin/webpack --progress --color",
    "watch:dev": "NODE_ENV=local ./node_modules/.bin/webpack --progress --color -w",
    "watch:prod": "NODE_ENV=prod ./node_modules/.bin/webpack --progress --color -w",
    "serve": "./node_modules/.bin/lite-server",
    "lint": "./node_modules/.bin/gulp tslint && npm run eslint",
    "eslint": "./node_modules/.bin/eslint --ignore-path .gitignore --ext js --fix . .config",
    "test": "npm run lint && npm run karma-local",
    "karma-local": "echo 'NODE_ENV=test ./node_modules/.bin/karma start .config/karma.conf.js'",
    "karma-sauce": "NODE_ENV=test ./node_modules/.bin/karma start .config/karma-sauce.conf.js",
    "karma-bstack": "NODE_ENV=test ./node_modules/.bin/karma start .config/karma-browserstack.conf.js",
    "e2e": " ./node_modules/.bin/protractor ./.config/protractor.conf.js",
    "e2e:live": " ./node_modules/.bin/protractor ./.config/protractor.conf.js --elementExplorer",
    "write-changelog": "./node_modules/.bin/conventional-changelog -i CHANGELOG.md -s -p angular"
  },
  "peerDependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
