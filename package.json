    "@types/marked": "0.0.28",
    "@types/node": "6.0.40",
    "@types/webpack": "1.12.34",
    "angulartics2": "1.1.9",
    "body-parser": "1.15.2",
    "codebird": "2.6.0",
    "contentful": "3.6.4",
