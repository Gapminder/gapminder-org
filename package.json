{
  "name": "gapminder.org",
  "version": "0.0.1",
  "private": true,
  "description": "gapminder.org angular2 client application",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/concurrently \"npm run watch:dev\" \"npm run serve\"",
    "postinstall": "./node_modules/.bin/concurrently \"npm run install:typings\" \"npm run update-webdriver\"",
    "install:typings": "./node_modules/.bin/typings install",
    "update-webdriver": "./node_modules/.bin/webdriver-manager update",
    "deploy": "npm run build:prod && npm run publish:gh-pages",
    "publish:gh-pages": "./tools/gh-pages-publish.js",
    "build:dev": "./node_modules/.bin/webpack --progress --color",
    "build:prod": "NODE_ENV=production ./node_modules/.bin/webpack --progress --color",
    "watch:dev": "./node_modules/.bin/webpack --progress --color -w",
    "watch:prod": "NODE_ENV=production ./node_modules/.bin/webpack --progress --color -w",
    "serve": "./node_modules/.bin/lite-server",
    "lint": "./node_modules/.bin/gulp tslint && npm run eslint",
    "eslint": "./node_modules/.bin/eslint --ignore-path .gitignore --ext js --fix . .config",
    "test": "npm run lint && npm run karma-local",
    "karma-local": "NODE_ENV=test ./node_modules/.bin/karma start .config/karma.conf.js",
    "karma-sauce": "NODE_ENV=test ./node_modules/.bin/karma start .config/karma-sauce.conf.js",
    "karma-bstack": "NODE_ENV=test ./node_modules/.bin/karma start .config/karma-browserstack.conf.js",
    "e2e": " ./node_modules/.bin/protractor ./.config/protractor.conf.js",
    "e2e:live": " ./node_modules/.bin/protractor ./.config/protractor.conf.js --elementExplorer",
    "write-changelog": "./node_modules/.bin/conventional-changelog -i CHANGELOG.md -s -p angular"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.9",
    "es6-promise": "3.1.2",
    "es6-shim": "0.35.0",
    "es7-reflect-metadata": "1.6.0",
    "marked": "0.3.5",
    "ng2-bootstrap": "1.0.5",
    "ng2-contentful": "0.0.8",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "compression-webpack-plugin": "0.2.0",
    "concurrently": "2.0.0",
    "conventional-changelog-cli": "1.1.1",
    "copy-webpack-plugin": "1.1.1",
    "coveralls": "2.11.8",
    "cpy": "4.0.0",
    "css-loader": "0.23.1",
    "del": "2.2.0",
    "eslint": "2.3.0",
    "eslint-config-valorsoft": "0.0.8",
    "exports-loader": "0.6.3",
    "file-loader": "0.8.5",
    "gh-pages": "0.11.0",
    "gulp": "3.9.1",
    "gulp-s3": "0.3.0",
    "gulp-s3-deploy": "1.0.1",
    "gulp-tslint": "4.3.3",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.10.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine": "2.4.1",
    "jasmine-spec-reporter": "2.4.0",
    "karma": "0.13.22",
    "karma-browserstack-launcher": "0.1.10",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.5",
    "karma-jasmine": "0.3.7",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sauce-launcher": "0.3.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "1.7.0",
    "lite-server": "2.1.0",
    "node-sass": "3.4.2",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.5",
    "pre-commit": "1.1.2",
    "protractor": "3.1.1",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.3",
    "sass-loader": "3.1.2",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.0",
    "stylus-loader": "1.5.1",
    "to-string-loader": "1.1.3",
    "ts-loader": "0.8.1",
    "tslint": "3.3.0",
    "typescript": "1.8.7",
    "typings": "0.7.7",
    "webpack": "1.12.14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gapminder/gapminder-org.git"
  },
  "keywords": [
    "gapminder"
  ],
  "author": "Dmitriy Shekhovtsov <valorkin@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Gapminder/gapminder-org/issues"
  },
  "homepage": "https://github.com/Gapminder/gapminder-org#readme"
}
