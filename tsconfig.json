{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "removeComments": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "listFiles": false,
    "noLib": false,
    "outDir": "dist",
    "stripInternal": true,
    "lib": ["dom", "es6"],
    "types": [
      "lodash",
      "marked",
      "node",
      "webpack"
    ]
  },
  "exclude": [
    "node_modules", "bundles", "dist"
  ],
  "atom": { "rewriteTsconfig": false },
  "files": [
   ]
}
